{
	"name": "durst_wt",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"prepare": "husky install",
		"format": "prettier -w . && xo --fix .",
		"lint": "prettier -c . && xo",
		"start": "node index.js",
		"deploy": "node deploy-commands.js"
	},
	"dependencies": {
		"@discordjs/builders": "^1.5.0",
		"@discordjs/rest": "^1.7.1",
		"@discordjs/voice": "^0.16.0",
		"discord-api-types": "^0.37.37",
		"discord.js": "^13",
		"dotenv": "^16.0.3",
		"jsdom": "^21.1.1",
		"node-bash-title": "^0.0.2",
		"util": "^0.12.5",
		"xmldom": "^0.6.0"
	},
	"devDependencies": {
		"husky": "^8.0.2",
		"lint-staged": "^13.0.4",
		"prettier": "^2.8.0",
		"xo": "^0.54.2"
	},
	"prettier": {
		"bracketSpacing": true,
		"singleQuote": false,
		"trailingComma": "es5",
		"useTabs": true
	},
	"xo": {
		"envs": [
			"es2021",
			"node"
		],
		"prettier": true,
		"rules": {
			"node/file-extension-in-import": "off",
			"n/file-extension-in-import": "off",
			"capitalized-comments": "off",
			"spaced-comment": "off",
			"unicorn/prefer-top-level-await": "off",
			"unicorn/no-await-expression-member": "off",
			"no-await-in-loop": "off"
		}
	},
	"lint-staged": {
		"*": "prettier -uw"
	},
	"engines": {
		"node": ">=16.6.0",
		"npm": ">=7.0.0"
	}
}
